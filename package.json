{
  "name": "rabi-ecommerce",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "test": "cross-env NODE_PATH=. jest",
    "test:integration": "npm test -- --runInBand -c jest-integration.config.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --ext .ts --fix",
    "migration": "cross-env NODE_PATH=../../../.. knex --knexfile src/frameworks/database/knex/knexfile.ts",
    "migration:make": "npm run migration migrate:make --",
    "migration:latest": "npm run migration migrate:latest --",
    "migration:down": "npm run migration migrate:down --",
    "migration:list": "npm run migration migrate:list --",
    "migration:rollback:all": "npm run migration migrate:rollback --all --",
    "migration:build": "./src/frameworks/database/knex/build-database.sh"
  },
  "author": "Kelvin Floresta de Andrade",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^26.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.167",
    "@types/pg": "^7.14.3",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.6",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "ajv": "^6.12.4",
    "bcrypt": "^5.0.1",
    "fastify": "^3.2.0",
    "fastify-cors": "^5.0.0",
    "json-schema": "^0.2.5",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "knex-paginate": "^1.2.3",
    "lodash": "^4.17.20",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.5.0",
    "uuid": "^8.3.0"
  }
}
